<ibx-res-bundle name="texteditor_bundle.xml">
	<ibx-package src="{context}/ibxtools/explore/explore_open_dialog.xml"/> <!-- Open/Save Dialog -->	
	<strings>
		<string-file src="{res}/com.ibi.intl.bip_editor.bip_editor"  default="true"/>
		<string-file src="{res}/com.ibi.intl.bip_editor.bip_RTeditor"  default="true"/>	
		<string-bundle>
		<![CDATA[
		]]>
		</string-bundle>
	</strings>

	<styles>
		<style-file src="./css/text_editor.css" loadContext="bundle"/>
	</styles>
	
	<scripts>
		<script-file src="./js/text_editor.js" loadContext="bundle"/>	
	</scripts>

	<markup>
		<markup-block>
			<div class="text-editor-resources">
				<div class="te-tool-bar" data-ibx-type="ibxHBox" data-ibxp-justify="start" data-ibxp-align="center">
					<div class="te-tb-new te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="tbNew" tabindex="1" data-menu-cmd="onNew" title="@ibxString('bid_menu_file_new')" data-ibxp-glyph="insert_drive_file" data-ibxp-glyph-classes="material-icons md-18"></div>
					<div class="te-tb-open te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="tbOpen" tabindex="2" data-menu-cmd="onOpen" title="@ibxString('bid_menu_file_open')" data-ibxp-glyph="folder_open" data-ibxp-glyph-classes="material-icons md-18"></div>
					<div class="te-tb-save te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="tbSave" tabindex="3" data-menu-cmd="onSave" title="@ibxString('bid_menu_file_save')" data-ibxp-glyph="save" data-ibxp-glyph-classes="material-icons md-18"></div>
					<div class="te-tb-run te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="tbRun" tabindex="4" data-menu-cmd="onRun" title="@ibxString('bid_run')" data-ibxp-glyph="play_arrow" data-ibxp-glyph-classes="material-icons md-18"></div>
				</div>
				<div class="te-menu-bar" data-ibx-type="ibxMenuBar">
					<div data-ibx-type="ibxMenuButton" data-ibx-name="menuFile" tabindex="5">@ibxString('bid_menu_file')
						<div class="te-file-menu ibx-menu-no-icons te-menu" data-ibx-type="ibxMenu">
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="fileNew">@ibxString('bid_menu_file_new')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="fileOpen">@ibxString('bid_menu_file_open')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="fileSave">@ibxString('bid_menu_file_save')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="fileSaveAs">@ibxString('bid_menu_file_saveas')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="fileClose">@ibxString('bid_menu_file_close')</div>
							<div class="fileExit" data-ibx-type="ibxMenuItem" data-menu-cmd="fileExit">@ibxString('bid_menu_file_exit')</div>
						</div>
					</div>
					<div data-ibx-type="ibxMenuButton" data-ibx-name="menuEdit" tabindex="6">@ibxString('bid_menu_edit')
						<div class="te-edit-menu ibx-menu-no-icons te-menu" data-ibx-type="ibxMenu">
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="editDelete">@ibxString('bid_menu_edit_delete')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="editSelectAll">@ibxString('bid_menu_edit_selectall')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="editUpperCase">@ibxString('bid_menu_edit_uppercase')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="editLowerCase">@ibxString('bid_menu_edit_lowercase')</div>
						</div>
					</div>
					<div data-ibx-type="ibxMenuButton" data-ibx-name="menuSearch">@ibxString('bid_menu_search')
						<div class="te-search-menu ibx-menu-no-icons te-menu" data-ibx-type="ibxMenu">
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="searchFind">@ibxString('bid_menu_search_find')</div>
							<div data-ibx-type="ibxMenuItem" data-menu-cmd="searchGoTo">@ibxString('bid_menu_search_goto')</div>
						</div>
					</div>
					<div class="te-menu-button" data-ibx-type="ibxMenuButton" data-ibx-name="menuOptions" data-menu-cmd="menuOptions">@ibxString('bid_menu_options')</div>
					<div class="te-menu-button" data-ibx-type="ibxMenuButton" data-ibx-name="menuHelp" data-menu-cmd="menuHelp">@ibxString('bid_menu_help')</div>
				</div>
				
				<!-- Go To Line -->				
				<div class="te-search-go-to-line goToLine" data-ibx-type="ibxHBox" data-ibxp-justify="start" data-ibxp-align="center">
					<div class="txtGoToLine" data-ibx-type="ibxTextField" tabindex="1"></div>
					<div class="ibx-primary btnGoToLine" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_OK')</div>
					<div class="ibx-default btnCloseGoToLine" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_close')</div>
				</div>	
			
				<div class="te-txt-area" data-ibx-type="ibxTextArea" data-ibx-name="txtArea" data-ibxp-default-focused="true"></div>
				
				<!-- Find/ Replace -->
				<div class="te-search-find-replace findReplace" data-ibx-type="ibxVBox" data-ibxp-justify="start" data-ibxp-align="left">
					
					<div class="te-search-find-replace-1" data-ibx-type="ibxHBox" data-ibxp-align="center">
						<div class="findLbl" data-ibx-type="ibxLabel">@ibxString('bid_findTextLbl')</div>
						<div class="findText" data-ibx-type="ibxTextField" tabindex="1"></div>
						<div class="ibx-default btnFindPrevious" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_findPrevBtnLbl')</div>
						<div class="ibx-default btnFind" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_findBtnLbl')</div>
						<div class="ibx-default btnReplace" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_replaceBtnLbl')</div>						
					</div>
					
					<div class="te-search-find-replace-2" data-ibx-type="ibxHBox" data-ibxp-align="center">
						<div class="replaceLbl" data-ibx-type="ibxLabel">@ibxString('bid_replaceTextLbl')</div>
						<div class="replaceText" data-ibx-type="ibxTextField" tabindex="1"></div>
						<div class="ibx-default btnReplaceAll" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_replaceAllBtnLbl')</div>
						<div class="ibx-default btnUndo" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_unDoBtnLbl')</div>
						<div class="ibx-default btnFindClose" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_close')</div>						
					</div>
					
					<div class="te-search-find-replace-3" data-ibx-type="ibxHBox" data-ibxp-align="center">
					<!--
						<div class="emptyLbl1" data-ibx-type="ibxLabel"></div>
						<div class="emptyLbl2" data-ibx-type="ibxLabel"></div>
					-->
			            <div class="cbMatchCase" data-ibx-type="ibxCheckBoxSimple" tabIndex="1">@ibxString('bid_matchCase')</div>
					</div>
				</div>
										
				<div class="te-status-bar" data-ibx-type="ibxHBox" data-ibxp-justify="start" data-ibxp-align="center">
					<div class="te-sb-line-marker-label" data-ibx-type="ibxLabel">@ibxString('bid_line')</div>
					<div class="te-sb-line-marker-value te-linePosLbl" data-ibx-type="ibxLabel"></div>
					<div class="te-sb-column-marker-label" data-ibx-type="ibxLabel">@ibxString('bid_column')</div>
					<div class="te-sb-column-marker-value te-columnPosLbl" data-ibx-type="ibxLabel"></div>
				</div>
			</div>
			
			
			<div class="text-editor-options" data-ibx-type="ibxDialog" data-ibxp-type="medium" data-ibx-name-root="true">
				<div class="ibx-dialog-main-box" data-ibx-type="ibxVBox" data-ibx-name="vbMain" data-ibxp-align="stretch">
					<div class="ibx-dialog-title-box" data-ibx-type="ibxHBox" data-ibx-name="titleBox" data-ibxp-align="center">
						<div class="ibx-title-bar-caption" data-ibx-type="ibxLabel" data-ibx-name="caption" data-ibxp-justify="start"></div>
						<div class="ibx-title-bar-close-button" tabIndex="1" data-ibx-type="ibxButton" data-ibx-name="titleClose"></div>
					</div>
					
					<div class="ibx-dialog-content" data-ibx-type="ibxVBox" data-ibx-name="contentBox" data-ibxp-justify="start" data-ibxp-align="stretch">
					
					
						<div class="te-options-srvChk srvChk" data-ibx-type="ibxCheckBoxSimple" tabIndex="1">@ibxString('bid_assign_server')</div>
						
						
						<div class="te-options-srvList srvList" data-ibx-type="ibxListBox" data-ibx-name="srvList" data-ibxp-align="center"></div>
						
	
						<div class="te-options-appChk appChk" data-ibx-type="ibxCheckBoxSimple" tabIndex="1">@ibxString('bid_assign_app')</div>
						
						
						<div class="te-options-appBtn ibx-default appBtn" data-ibx-type="ibxButton" data-ibxp-justify="center">@ibxString('bid_load_app_paths')</div>
						
	
						<div class="te-options-app-hbox" data-ibx-type="ibxGrid" data-ibxp-cols="1fr auto 1fr">
	
	
							
								<div class="te-options-avail-lbl" data-ibx-type="ibxLabel" data-ibx-col="1" data-ibx-row="1">@ibxString('bid_available')</div>
								
								<div class="te-options-list-wrapper" data-ibx-type="ibxHBox" data-ibx-col="1" data-ibx-row="2" data-ibxp-justify="stretch" data-ibxp-align="stretch">
									<div class="te-options-avlapplist avlAppList" data-ibx-name="avlAppList" data-ibx-type="ibxList" data-ibxp-multi-select="true"></div>		
								</div>				
								
							
							
								<div class="te-options-sel-del-app-btns" data-ibx-type="ibxVBox" data-ibx-col="2" data-ibx-row="2" data-ibxp-justify="center" data-ibxp-align="center">
									<div class="te-options-selAppBtn selAppBtn te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="selAppBtn" tabindex="1" data-menu-cmd="selAppBtn" title="@ibxString('bid_select_app')" data-ibxp-glyph="arrow_forward" data-ibxp-glyph-classes="material-icons md-18"></div>
									<div class="te-options-delAppBtn delAppBtn te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="delAppBtn" tabindex="2" data-menu-cmd="delAppBtn" title="@ibxString('bid_do_not_select_app')" data-ibxp-glyph="arrow_back" data-ibxp-glyph-classes="material-icons md-18"></div>
								</div>
							
		
							
								<div class="te-options-sel-header" data-ibx-type="ibxHBox" data-ibx-col="3" data-ibx-row="1" data-ibxp-justify="spaceBetween" data-ibxp-align="center">
								
									<div class="te-options-sel-lbl" data-ibx-type="ibxLabel">@ibxString('bid_selected')</div>
									
									<div class="te-options-ap-down-app-btns" data-ibx-type="ibxHBox" data-ibxp-justify="start" data-ibxp-align="right">
										<div class="te-options-appUp appUp te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="appUp" tabindex="1" data-menu-cmd="onAppUp" title="@ibxString('bid_appUp')" data-ibxp-glyph="arrow_drop_up" data-ibxp-glyph-classes="material-icons md-18"></div>
										<div class="te-options-appDown appDown te-tb-btn" data-ibx-type="ibxButtonSimple" data-ibx-name="appDown" tabindex="2" data-menu-cmd="onAppDown" title="@ibxString('bid_appDown')" data-ibxp-glyph="arrow_drop_down" data-ibxp-glyph-classes="material-icons md-18"></div>
									</div>
								</div>
							
								<div class="te-options-list-wrapper" data-ibx-type="ibxHBox" data-ibx-col="3" data-ibx-row="2" data-ibxp-justify="stretch" data-ibxp-align="stretch">
							
									<div class="te-options-selapplist selAppList" data-ibx-name="selAppList" data-ibx-type="ibxList" data-ibxp-multi-select="true"></div>
								</div>

						</div>
	
						<div class="te-options-paramPrompt paramPrompt" data-ibx-type="ibxCheckBoxSimple" tabIndex="1">@ibxString('bid_promptParm')</div>
						
					</div>
					
					<div class='ibx-dialog-button-box' data-ibx-type='ibxHBox' data-ibx-name='btnBox' data-ibxp-justify='end' data-ibxp-align='center'>
						<div class='ibx-dialog-ok-button' data-ibx-type='ibxButton' data-ibx-name='btnOK' data-ibxp-text="@ibxString('bid_OK')" data-ibxp-icon-position='top' tabIndex='1'></div>
						<div class='ibx-dialog-apply-button' data-ibx-type='ibxButton' data-ibx-name='btnApply' data-ibxp-text="@ibxString('bid_reset')" data-ibxp-icon-position='top' tabIndex='1'></div>
						<div class='ibx-dialog-cancel-button' data-ibx-type='ibxButton' data-ibx-name='btnCancel' data-ibxp-text="@ibxString('bid_cancel')" data-ibxp-icon-position='top' tabIndex='1'></div>
					</div>
				</div>
			</div>
