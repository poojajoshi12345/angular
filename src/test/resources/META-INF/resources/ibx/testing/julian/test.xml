<bean id="fontawesome" class="com.ibi.webapp.license.ThirdPartyJar" singleton="true">
	<constructor-arg index="0">
		<value>
			Font Awesome
		</value>
		<!-- Description -->
	</constructor-arg>
	<constructor-arg index="1">
		<value>4.7.0</value>
		<!-- version -->
	</constructor-arg>
	<constructor-arg index="2">
		<!-- jars -->
		<list>
			<value>font-awesome.css</value>
		</list>
	</constructor-arg>
	<constructor-arg index="3">
		<value>OFL 1.1</value>
		<!-- license_type -->
	</constructor-arg>
	<constructor-arg index="4">
		<!-- license_file -->
		<list>
			<value>bootstrap.js_license.txt</value>
		</list>
	</constructor-arg>
	<constructor-arg index="5">
		<list>
			<value>http://getbootstrap.com/getting-started/#license-faqs</value>
			<!-- links -->
		</list>
	</constructor-arg>
	<constructor-arg index="6">
		<value>false</value>
		<!-- hide from i5 -->
	</constructor-arg>
	<constructor-arg index="7">
		<!-- when updating a js, or a jar, please add the History of patches about the file, just the filename version number and date and time -->
		<list>
			<value> bootstrap.js v1.3, 2016-12-26 13:23:53, 8.0.01</value>

		</list>
	</constructor-arg>
</bean>