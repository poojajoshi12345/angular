<?xml version="1.0" encoding="UTF-8"?>
<ibx-res-bundle>
	<strings>
		<string-bundle>
		<![CDATA[
		{
			"language":"en",
			"strings":
			{
				"ITEM_TOOLTIP1":"title ='Title=\t\t {1}\nPath=\t\t {2}\nLast Modified=\t {3}'", 
				"ITEM_TOOLTIP2":"title ='Title=\t\t {1}\nLast Modified=\t {2}'",
				"markup.search":"Search {1}"
			}
		}		
		]]>
		</string-bundle>
	</strings>

	<styles>
		<style-sheet>
		</style-sheet>		
		<style-file src="../../../../homepage/resources/css/itemelements.css" loadContext="bundle"/>
		<style-file src="./opensavedialog.css" loadContext="bundle"/>				
	</styles>

	<markup>
		<markup-block>	
			<div class="save-dialog-resources" data-ibx-name-root="true" data-ibx-type="opensavedialog" data-ibxp-auto-size="false">
				
				<div class="sd-toolbar" data-ibx-type="ibxHBox" data-ibxp-align="stretch" data-ibxp-wrap="false">
						<div class="sd-crumb-box" data-ibx-name="_breadCrumbTrail" data-ibx-type="breadCrumbTrail" data-ibxp-align="center">										
						</div>
						<div class="sd-toolbar-spacer"></div> 
						<div class="sd-div-search" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
							<div class="sd-btn-search" data-ibx-type="ibxLabel" data-ibxp-glyph="search" data-ibxp-glyph-classes="material-icons"></div>
							<div class="sd-txt-search" data-ibx-type="ibxTextField" data-ibxp-placeholder="Search..."></div>
							<div class="sd-btn-clear-search" data-ibx-type="ibxButtonSimple" data-ibxp-glyph-classes="fa fa-remove" title="Clear Search"></div>							
						</div>
						
						<div class="sd-toolbar-button-divset" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
							<div class="sd-toolbar-button-div" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
								<div class="sd-btn-how-view-grid" data-ibx-name="sdViewList" data-ibx-type="ibxButtonSimple"  title="List View" data-ibxp-glyph-classes="fa fa-list"></div>
								<div class="sd-btn-how-view" data-ibx-name="sdViewTiles" data-ibx-type="ibxButtonSimple"  title="Tile View" data-ibxp-glyph-classes="fa fa-th"></div>
							</div>	
							<div class="sd-toolbar-button-div" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
								<div class="sd-btn-refresh" data-ibx-type="ibxButtonSimple" title="Refresh" data-ibxp-glyph="autorenew" data-ibxp-glyph-classes="material-icons"></div>
							</div>
						</div>				
				</div>
				
				
						
			
				<div class="sd-files-box-files"  data-ibx-type="ibxHBox" data-ibx-name="tilesBox" data-ibxp-wrap="true" ></div>
				<div class="sd-files-listing" data-ibx-type="ibxVBox" data-ibx-name="listBox" data-ibxp-align="stretch" ></div>
			
		


				<div class="sd-text-input" data-ibx-type="ibxVBox" data-ibxp-align="stretch" >
					<div class="sd-input-div" data-ibx-type="ibxHBox" >
						<div data-ibx-type="ibxLabel" class="sd-form-field-title" data-ibxp-align="center" data-ibxp-text-align="right" data-ibxp-justify="center" data-ibxp-text="Title"></div>
						<div class="sd-content-title-spacer"></div>
						<div data-ibx-type="ibxTextField" data-ibx-name="sdtxtFileTitle"  class="sd-form-field-text-title"  data-ibxp-align="center" data-ibxp-placeholder="Enter Title Here" ></div>
					</div>
					<div class="sd-input-div" data-ibx-type="ibxHBox" >
						<div data-ibx-type="ibxLabel" class="sd-form-field-title" data-ibxp-text-align="center" data-ibxp-align="center" data-ibxp-justify="center"  data-ibxp-text="Name"></div>
						<div class="sd-content-title-spacer"></div>
						<div data-ibx-type="ibxTextField" data-ibx-name="sdtxtFileName" class="sd-form-field-text-name"  data-ibxp-align="center" data-ibxp-placeholder="Enter Name Here" ></div>
					</div>
				</div>					
				
					
			</div>	
		</markup-block>
		<markup-block>
			<div class="content-title-label-files" data-ibx-type="ibxHBox" data-ibxp-align="center">
				<div class="content-title-label" data-ibx-type="ibxLabel" data-ibxp-text="Files"></div>
				<div class="content-title-spacer"></div>
				<div class="content-title-buttons" data-ibx-type="ibxHBox" data-ibxp-align="right">
			 	<div class="content-title-btn-name" data-ibx-type="ibxButton" data-ibxp-text="Title" data-ibxp-icon-position="right"></div>
			 	<div class="content-title-btn-arrow" data-ibx-type="ibxButton" title="str_reverse_sort_order" data-ibxp-icon-position="right" data-ibxp-glyph="arrow_upward" data-ibxp-glyph-classes="material-icons"></div>
				</div>
			</div>
			<div class="content-title-label-folders" data-ibx-type="ibxHBox" data-ibxp-align="center">
				<div class="content-title-label" data-ibx-type="ibxLabel" data-ibxp-text="Folders"></div>
				<div class="content-title-spacer"></div>
				<div class="content-title-buttons" data-ibx-type="ibxHBox" data-ibxp-align="right">
			 	<div class="content-title-btn-name" data-ibx-type="ibxButton" data-ibxp-text="Title" ></div>
			 	<div class="content-title-btn-arrow" data-ibx-type="ibxButton"  title="str_reverse_sort_order" data-ibxp-icon-position="right" data-ibxp-glyph="arrow_upward" data-ibxp-glyph-classes="material-icons"></div>
				</div>
			</div>
		</markup-block>
		<markup-block>
		<div class="right-carat" data-ibx-type="ibxButtonSimple" data-ibxp-glyph-classes="fa fa-chevron-right"></div>
		</markup-block>
	</markup>

	<scripts>
		<script-file src="./opensavedialog.js" loadContext="bundle"/>
		<script-file src="../../../../homepage/resources/js/items_home.js" loadContext="bundle"/>
		<script-file src="../../../../homepage/resources/js/views_home.js" loadContext="bundle"/>
		<script-file src="../../../../homepage/resources/js/itemboxes_control_home.js" loadContext="bundle"/>
		<script-file src="../../../../homepage/resources/js/filegrid.js" loadContext="bundle"/>
		<script-file src="../../../../homepage/resources/js/breadcrumbtrail.js" loadContext="bundle"/>
		<script-file src="../../../../homepage/resources/js/search_home.js" loadContext="bundle"/>
		<script-block>
		<![CDATA[
			
			
			
			
			
		]]>
		</script-block>
	</scripts>
</ibx-res-bundle>
