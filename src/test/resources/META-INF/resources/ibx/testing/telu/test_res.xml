<ibx-res-bundle name="pd-res-shared.xml" loadContext="bundle">
  
<ibx-res-bundle src="/ibi_apps/ibxtools/shared_resources/shared_resource_bundle.xml"/>
    
  <markup>
    <markup-block> 
      <div data-ibx-type="ibxDialog" class="pd-warning-dirty" data-ibxp-auto-close="false" data-ibxp-modal="true" data-ibxp-type="medium warning" data-ibxp-buttons="" data-ibxp-moveable="false" data-ibxp-default-action=".pd-warning-dirty-yes" data-ibxp-opaque="true">
        <div data-ibx-type="ibxVBox" data-ibxp-align="stretch">
          <div data-ibx-type="ibxLabel" class="pd-warning-dirty-label" data-ibxp-justify="start" data-ibxp-text-wrap="normal">Message...</div>
          <div data-ibx-type="ibxHBox" data-ibxp-justify="end">
            <div data-ibx-type="ibxButton" class="pd-warning-dirty-button pd-warning-dirty-yes ibx-primary" data-ibxp-justify="center">Yes</div>
            <div data-ibx-type="ibxButton" class="pd-warning-dirty-button pd-warning-dirty-no ibx-default" data-ibxp-justify="center">No</div>
            <div data-ibx-type="ibxButton" class="pd-warning-dirty-button pd-warning-dirty-cancel ibx-default" data-ibxp-justify="center">Cancel</div>
          </div>
        </div>
      </div>
    </markup-block> 
  </markup>

</ibx-res-bundle>