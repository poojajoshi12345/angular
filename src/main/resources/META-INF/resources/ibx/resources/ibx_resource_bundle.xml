				<color idx="12" display-name="" value="#b97a57" opacity="1"/>
				<color idx="3" display-name="" value="#ed1c24" opacity="1"/>
				<color idx="13" display-name="" value="#ffaec9" opacity="1"/>
				<color idx="4" display-name="" value="#ff7827" opacity="1"/>
				<color idx="14" display-name="" value="#ffc90e" opacity="1"/>
				<color idx="5" display-name="" value="#fff200" opacity="1"/>
				<color idx="15" display-name="" value="#efe4b0" opacity="1"/>
				<color idx="6" display-name="" value="#22b14c" opacity="1"/>
				<color idx="16" display-name="" value="#b5e61d" opacity="1"/>
				<color idx="7" display-name="" value="#00a2e8" opacity="1"/>
				<color idx="17" display-name="" value="#99d9ea" opacity="1"/>
				<color idx="8" display-name="" value="#3f48cc" opacity="1"/>
				<color idx="18" display-name="" value="#7092be" opacity="1"/>
				<color idx="9" display-name="" value="#a349a4" opacity="1"/>
				<color idx="19" display-name="" value="#c8bfe7" opacity="1"/>
				<custom_colors>
					<color idx="0" display-name="" value="#ffffff" opacity="1"/>
					<color idx="1" display-name="" value="#ffffff" opacity="1"/>
					<color idx="2" display-name="" value="#ffffff" opacity="1"/>
					<color idx="3" display-name="" value="#ffffff" opacity="1"/>
					<color idx="4" display-name="" value="#ffffff" opacity="1"/>
					<color idx="5" display-name="" value="#ffffff" opacity="1"/>
					<color idx="6" display-name="" value="#ffffff" opacity="1"/>
					<color idx="7" display-name="" value="#ffffff" opacity="1"/>
					<color idx="8" display-name="" value="#ffffff" opacity="1"/>
					<color idx="9" display-name="" value="#ffffff" opacity="1"/>
				</custom_colors>
			</palette>
		</markup-block>

		<markup-block>
			<div class="pgrid-prop-border-editor-template">
				<div tabindex="0" class="pgrid-prop-border-ctrl" data-ibx-name="labelWidth" data-ibx-type="ibxLabel" data-ibxp-for=".pgrid-prop-border-width">Width</div>
				<div tabindex="0" class="pgrid-prop-border-ctrl pgrid-prop-spinner pgrid-prop-border-width" data-ibx-name="spinnerWidth" data-ibx-type="ibxSpinner"></div>
				
				<div tabindex="0" class="pgrid-prop-border-ctrl pgrid-prop-border-menu-button" data-ibx-name="btnStyle" data-ibx-type="ibxSelectMenuButton" data-ibxp-use-value-as-text="true">Style
					<div style="border:none" class="pgrid-prop-border-style-menuitem"  data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="none">None</div>
					<div style="border:dashed" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="dashed">Dashed</div>
					<div style="border:dotted" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="dotted">Dotted</div>
					<div style="border:double" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="double">Double</div>
					<div style="border:groove" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="groove">Groove</div>
					<div style="border:hidden" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="hidden">Hidden</div>
					<div style="border:inset" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="inset">Inset</div>
					<div style="border:outset" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="outset">Outset</div>
					<div style="border:ridge" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="ridge">Ridge</div>
					<div style="border:solid" class="pgrid-prop-border-style-menuitem" data-ibx-type="ibxCheckMenuItem" data-ibxp-user-value="solid">Solid</div>
				</div>
				
				<div tabindex="0" class="pgrid-prop-border-ctrl pgrid-prop-border-menu-button" data-ibx-name="btnColor" data-ibx-type="ibxMenuButton" data-ibxp-use-value-as-text="true" data-ibxp-glyph-classes="pgrid-color-picker-swatch">Color
					<div data-ibx-type="ibxMenu" data-ibxp-destroy-on-close="false">
						<div tabindex="0" class="ibx-menu-item pgrid-color-picker-popup" data-ibx-name="colorPicker" data-ibx-type="ibxColorPicker" data-ibxp-set-opacity="false"></div>
					</div>
				</div>
			</div>
		</markup-block>

	</markup>

