	<!--markup-->
	<markup>
		<!--inline-->
		<markup-block>
			<div class="ibx-dialog-template">
				<div class="ibx-dialog-main-box" data-ibx-type="ibxVBox" data-ibx-name="vbMain" data-ibxp-align="stretch">
					<div class="ibx-dialog-title-box" data-ibx-type="ibxHBox" data-ibx-name="titleBox" data-ibxp-align="center">
						<div class="ibx-title-bar-caption" data-ibx-type="ibxLabel" data-ibx-name="caption" data-ibxp-justify="start"></div>
						<div class="ibx-title-bar-close-button" data-ibx-type="ibxButton" data-ibx-name="titleClose" title='@ibxString("IBX_STR_CLOSE")'></div>
					</div>
					<div class="ibx-dialog-content" data-ibx-type="ibxVBox" data-ibx-name="contentBox" data-ibxp-align="stretch"></div>
					<div class='ibx-dialog-button-box' data-ibx-type='ibxHBox' data-ibx-name='btnBox' data-ibxp-align='stretch'>
						<div class="ibx-dialog-button-padding" data-ibx-type="ibxNull" data-ibx-name="btnPadding"></div>
						<div class='ibx-dialog-ok-button' data-ibx-type='ibxButton' data-ibx-name='btnOK' data-ibxp-text='@ibxString("IBX_STR_OK")' tabIndex='0'></div>
						<div class='ibx-dialog-no-button' data-ibx-type='ibxButton' data-ibx-name='btnNo' data-ibxp-text='@ibxString("IBX_STR_NO")' tabIndex='0'></div>
						<div class='ibx-dialog-apply-button' data-ibx-type='ibxButton' data-ibx-name='btnApply' data-ibxp-text='@ibxString("IBX_STR_APPLY")' tabIndex='0'></div>
						<div class='ibx-dialog-cancel-button' data-ibx-type='ibxButton' data-ibx-name='btnCancel' data-ibxp-text='@ibxString("IBX_STR_CANCEL")' tabIndex='0'></div>
					</div>
				</div>
			</div>
			<div class="ibx-carousel-template">
				<div class="ibx-csl-items-container" data-ibx-name="_itemsContainer" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
					<div tabindex="0" class="csl-scroll-btn" data-ibxp-auto-focused="true" title="@ibxString('IBX_CAROUSEL_PREV')" data-ibx-name="_prevBtn" data-ibx-type="ibxButtonSimple" data-ibxp-class="ibx-csl-prev-btn"></div>
					<div tabindex="0" class="ibx-csl-items-box" data-ibx-name="_itemsBox" data-ibx-type="ibxHBox" data-ibxp-nav-key-root="true" data-ibxp-nav-key-reset-focus-on-blur="false" data-ibxp-focus-default="true" data-ibxp-align="center"></div>
					<div tabindex="0" class="csl-scroll-btn" title="@ibxString('IBX_CAROUSEL_NEXT')" data-ibx-name="_nextBtn" data-ibx-type="ibxButtonSimple" data-ibxp-class="ibx-csl-next-btn"></div>
				</div>
				<div tabindex="0" class="ibx-csl-page-markers" data-ibx-name="_pageMarkers" data-ibx-type="ibxHBox" data-ibxp-nav-key-root="true" data-ibxp-focus-default="true" data-ibxp-nav-key-reset-focus-on-blur="false" data-ibxp-justify="center" data-ibxp-align="center">
				</div>
			</div>
		</markup-block>
		<markup-block class="ibx-palette-picker-template">
			<div tabindex="0" class="pp-palette-select" data-ibx-type="ibxSelect" data-ibxp-readonly="true" data-ibx-name="_palSelect"></div>
			<div class="pp-swatch-box" data-ibx-type="ibxHBox" data-ibx-name="_swatchBox" data-ibxp-align="start" data-ibxp-justify="spaceAround" data-ibxp-wrap="true"></div>
			<div class="pp-custom-label" data-ibx-type="ibxLabel" data-ibx-name="_customLabel">@ibxString(PP_CUSTOM_COLORS)</div>
			<div class="pp-custom-box" data-ibx-type="ibxHBox" data-ibx-name="_customBox" data-ibxp-align="start" data-ibxp-justify="spaceAround" data-ibxp-wrap="true"></div>
			<div class="pp-no-fill-box" data-ibx-type="ibxHBox" data-ibx-name="_noFillBox" data-ibxp-align="center">
				<div tabindex="0" class="pp-swatch pp-no-fill" data-ibx-name="_nfSwatch" data-pp-hex-value="transparent" data-pp-rgb-value="transparent"></div>
				<div data-ibx-type="ibxLabel">@ibxString(PP_NO_FILL)</div>
			</div>
			<div class="pp-transparency-box" data-ibx-type="ibxVBox" data-ibx-name="_transparencyBox" data-ibxp-align="stretch">
				<div class="pp-transparency-label" data-ibx-type="ibxLabel">@ibxString(PP_TRANSPARENCY)</div>
				<div class="pp-transparency-slider" data-ibx-type="ibxSlider" data-ibx-name="_transSlider" data-ibxp-value-text-pos="end" data-ibxp-min-text-pos="center" data-ibxp-max-text-pos="center" data-ibxp-min="0" data-ibxp-max="100" data-ibxp-value="100"></div>
			</div>
		</markup-block>
		<markup-block class="ibx-palette-picker-default-palettes">
			<palette pid="default_basic" display-name="@ibxString(PP_PAL_NAME_BASIC)">
				<color idx="0" display-name="" value="#000000" opacity="1"/>
				<color idx="10" display-name="" value="#ffffff" opacity="1"/>
				<color idx="1" display-name="" value="#7f7f7f" opacity="1"/>
				<color idx="11" display-name="" value="#c3c3c3" opacity="1"/>
