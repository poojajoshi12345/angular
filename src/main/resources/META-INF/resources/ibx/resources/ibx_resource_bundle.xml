	</res-bundles>

	<!--strings-->
	<strings>
		<string-file src="./strings/strings.en.ibx.txt"/>

		<!--inline-->
		<string-bundle>
			<![CDATA[
		{
			"language":"en",
			"strings":
			{
			}
		}
		]]>
		</string-bundle>
	</strings>

	<!--styles-->
	<styles>
		<style-file src="./etc/jquery/jquery-ui-1.12.1/jquery-ui.css"/>
		<style-file src="./etc/gridstack/gridstack.css"/>
		<style-file src="./etc/material-icons/iconfont/material-icons.css"/>
		<style-file src="./etc/google/css/hind.css"/>
		<style-file src="./etc/google/css/lato.css"/>
		<style-file src="./etc/google/css/open-sans.css"/>
		<style-file src="./etc/google/css/roboto.css"/>
		<style-file src="./etc/font-awesome/css/font-awesome.css"/>

		<style-file src="./css/ibx-font/css/ibx-font.css"/>

		<style-file src="./css/widget.ibx.css"/>
		<style-file src="./css/collapsible.ibx.css"/>
		<style-file src="./css/flexbox.ibx.css"/>
		<style-file src="./css/grid.ibx.css"/>
		<style-file src="./css/splitter.ibx.css"/>
		<style-file src="./css/label.ibx.css"/>
		<style-file src="./css/progress.ibx.css"/>
		<style-file src="./css/control.ibx.css"/>
		<style-file src="./css/text.ibx.css"/>
		<style-file src="./css/textarea.ibx.css"/>
		<style-file src="./css/spinner.ibx.css"/>
		<style-file src="./css/button.ibx.css"/>
		<style-file src="./css/slider.ibx.css"/>
		<style-file src="./css/popup.ibx.css"/>
		<style-file src="./css/dialog.ibx.css"/>
		<style-file src="./css/form.ibx.css"/>
		<style-file src="./css/tabs.ibx.css"/>
		<style-file src="./css/accordion.ibx.css"/>
		<style-file src="./css/menu.ibx.css"/>
		<style-file src="./css/groupbox.ibx.css"/>
		<style-file src="./css/gridstack.ibx.css"/>
		<style-file src="./css/select.ibx.css"/>
		<style-file src="./css/iframe.ibx.css"/>
		<style-file src="./css/carousel.ibx.css"/>
		<style-file src="./css/datepicker.ibx.css"/>
		
		<!--inline-->
		<style-sheet name="">
		<![CDATA[
			.ibfs-error-dialog
			{
				width:450px;
				height:300px;
			}
			.ibfs-error-dialog > .ibx-dialog-main-box
			{
				position:absolute;
				left:0px;
				top:0px;
				right:0px;
				bottom:0px
			}
			.ibfs-error-details-label
			{
				margin-top:6px;
				margin-bottom:2px;
				font-weight:bold;
			}
			.ibfs-error-details
			{
				flex:1 1 auto;
				font-family:courier new;
				font-size:11px;
				margin-bottom:6px;
			}
		]]>
		</style-sheet>
	</styles>

	<!--markup-->
	<markup>
		<!--inline-->
		<markup-block>
			<div class="dialog-template" data-ibx-type="ibxDialog" data-ibx-name-root="true" data-ibxp-default-focused="cancel">
				<div class="ibx-dialog-main-box" data-ibx-type="ibxVBox" data-ibx-name="vbMain" data-ibxp-align="stretch">
					<div class="ibx-dialog-title-box" data-ibx-type="ibxHBox" data-ibx-name="titleBox" data-ibxp-align="center">
						<div class="ibx-title-bar-caption" data-ibx-type="ibxLabel" data-ibx-name="caption" data-ibxp-justify="start"></div>
						<div class="ibx-title-bar-close-button" tabIndex="1" data-ibx-type="ibxButton" data-ibx-name="titleClose"></div>
					</div>
					<div class="ibx-dialog-content" data-ibx-type="ibxVBox" data-ibx-name="contentBox" data-ibxp-align="stretch"></div>
					<div class='ibx-dialog-button-box' data-ibx-type='ibxHBox' data-ibx-name='btnBox' data-ibxp-justify='end' data-ibxp-align='center'>
						<div class='ibx-dialog-ok-button' data-ibx-type='ibxButton' data-ibx-name='btnOK' data-ibxp-text="OK" data-ibxp-icon-position='top' tabIndex='1'></div>
						<div class='ibx-dialog-apply-button' data-ibx-type='ibxButton' data-ibx-name='btnApply' data-ibxp-text="Apply" data-ibxp-icon-position='top' tabIndex='1'></div>
						<div class='ibx-dialog-cancel-button' data-ibx-type='ibxButton' data-ibx-name='btnCancel' data-ibxp-text="Cancel" data-ibxp-icon-position='top' tabIndex='1'></div>
					</div>
				</div>
			</div>

			<div class="ibfs-dlg-error">
				<div class="ibfs-error-details-label" data-ibx-type="ibxLabel">@ibxString("IDS_IBFS_ERROR_DETAILS_LABEL")</div>
				<div class="ibfs-error-details" data-ibx-name="_errDetails" data-ibx-type="ibxTextArea" data-ibxp-text-wrap="off"></div>
			</div>

			<div class="res-ibx-carousel-body">
				<div class="ibx-csl-items-container" data-ibx-name="_itemsContainer" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
					<div class="ibx-csl-prev-btn" tabIndex="0" data-ibx-name="_prevBtn" data-ibx-type="ibxButtonSimple"></div>
					<div class="ibx-csl-items-box" data-ibx-name="_itemsBox" data-ibx-type="ibxHBox" data-ibxp-align="center"></div>
					<div class="ibx-csl-next-btn" tabIndex="0" data-ibx-name="_nextBtn" data-ibx-type="ibxButtonSimple"></div>
				</div>
				<div class="ibx-csl-page-markers" tabIndex="0" data-ibx-name="_pageMarkers" data-ibx-type="ibxHBox" data-ibxp-justify="center" data-ibxp-align="center">
				</div>
			</div>
		</markup-block>
	</markup>

	<!--scripts-->
	<scripts>
		<!--PRELOADS FIRST-->
		<script-file src="./js/util.ibx.js"/>
		<script-file src="./js/ibxPreLoad.ibx.js"/>

		<!--POLYFILL/SHIM AND PLUGINS-->
		<script-file src="./etc/resizepolyfill/ResizeSensor.js"/>
		<script-file src="./etc/html2canvas/html2canvas.js"/>
		<script-file src="./etc/scrollwidthpolyfill/scrollwidthpolyfill.js"/>
		<script-file src="./etc/gridstack/underscore.js"/>
