		<script-file src="./js/label.ibx.js"/>
		<script-file src="./js/progress.ibx.js"/>
		<script-file src="./js/control.ibx.js"/>
		<script-file src="./js/text.ibx.js"/>
		<script-file src="./js/textarea.ibx.js"/>
		<script-file src="./js/spinner.ibx.js"/>
		<script-file src="./js/button.ibx.js"/>
		<script-file src="./js/slider.ibx.js"/>
		<script-file src="./js/popup.ibx.js"/>
		<script-file src="./js/dialog.ibx.js"/>
		<script-file src="./js/radiogroup.ibx.js"/>
		<script-file src="./js/form.ibx.js"/>
		<script-file src="./js/tabs.ibx.js"/>
		<script-file src="./js/accordion.ibx.js"/>
		<script-file src="./js/menu.ibx.js"/>
		<script-file src="./js/groupbox.ibx.js"/>
		<script-file src="./js/gridstack.ibx.js"/>
		<script-file src="./js/select.ibx.js"/>
		<script-file src="./js/iframe.ibx.js"/>
		<script-file src="./js/carousel.ibx.js"/>
		<script-file src="./js/datepicker.ibx.js"/>
		
		<!--scripts-->
		<script-block name="">
		<![CDATA[
		]]>
		</script-block>
	</scripts>

	<!--default external bundles to be loaded AFTER this bundles is fully loaded and running-->
	<packages>
	</packages>

</ibx-res-bundle>
