		<!--inline-->
		<style-sheet name="">
		<![CDATA[
			.ibfs-error-dialog
			{
				width:450px;
				height:300px;
			}
			.ibfs-error-dialog > .ibx-dialog-main-box
			{
				position:absolute;
				left:0px;
				top:0px;
				right:0px;
				bottom:0px
			}
			.ibfs-error-details-label
			{
				margin-top:6px;
				margin-bottom:2px;
				font-weight:bold;
			}
			.ibfs-error-details
			{
				flex:1 1 auto;
				font-family:courier new;
				font-size:11px;
				margin-bottom:6px;
			}
		]]>
		</style-sheet>
	</styles>

	<!--markup-->
	<markup>
		<!--inline-->
		<markup-block>
			<div class="dialog-template" data-ibx-type="ibxDialog" data-ibx-name-root="true" data-ibxp-default-action=".ibx-dialog-ok-button, cancel">
				<div class="ibx-dialog-main-box" data-ibx-type="ibxVBox" data-ibx-name="vbMain" data-ibxp-align="stretch">
					<div class="ibx-dialog-title-box" data-ibx-type="ibxHBox" data-ibx-name="titleBox" data-ibxp-align="center">
						<div class="ibx-title-bar-caption" data-ibx-type="ibxLabel" data-ibx-name="caption" data-ibxp-justify="start"></div>
						<div class="ibx-title-bar-close-button" data-ibx-type="ibxButton" data-ibx-name="titleClose" title='@ibxString("IBX_STR_CLOSE")'></div>
					</div>
					<div class="ibx-dialog-content" data-ibx-type="ibxVBox" data-ibx-name="contentBox" data-ibxp-align="stretch"></div>
					<div class='ibx-dialog-button-box' data-ibx-type='ibxHBox' data-ibx-name='btnBox' data-ibxp-align='stretch'>
						<div class="ibx-dialog-button-padding" data-ibx-name="btnPadding"></div>
						<div class='ibx-dialog-ok-button' data-ibx-type='ibxButton' data-ibx-name='btnOK' data-ibxp-text='@ibxString("IBX_STR_OK")' tabIndex='0'></div>
						<div class='ibx-dialog-no-button' data-ibx-type='ibxButton' data-ibx-name='btnNo' data-ibxp-text='@ibxString("IBX_STR_NO")' tabIndex='0'></div>
						<div class='ibx-dialog-apply-button' data-ibx-type='ibxButton' data-ibx-name='btnApply' data-ibxp-text='@ibxString("IBX_STR_APPLY")' tabIndex='0'></div>
						<div class='ibx-dialog-cancel-button' data-ibx-type='ibxButton' data-ibx-name='btnCancel' data-ibxp-text='@ibxString("IBX_STR_CANCEL")' tabIndex='0'></div>
					</div>
				</div>
			</div>

			<div class="ibfs-dlg-error">
				<div class="ibfs-error-details-label" data-ibx-type="ibxLabel">@ibxString("IDS_IBFS_ERROR_DETAILS_LABEL")</div>
				<div class="ibfs-error-details" data-ibx-name="_errDetails" data-ibx-type="ibxTextArea" data-ibxp-text-wrap="off"></div>
			</div>

			<div class="res-ibx-carousel-body">
				<div class="ibx-csl-items-container" data-ibx-name="_itemsContainer" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
					<div class="csl-scroll-btn" tabIndex="0" title="@ibxString('IBX_CAROUSEL_PREV')" data-ibx-name="_prevBtn" data-ibx-type="ibxButtonSimple" data-ibxp-class="ibx-csl-prev-btn"></div>
					<div class="ibx-csl-items-box" tabIndex="0" data-ibx-name="_itemsBox" data-ibx-type="ibxHBox" data-ibxp-nav-key-root="true" data-ibxp-nav-key-auto-focus="true" data-ibxp-align="center"></div>
					<div class="csl-scroll-btn" tabIndex="0" title="@ibxString('IBX_CAROUSEL_NEXT')" data-ibx-name="_nextBtn" data-ibx-type="ibxButtonSimple" data-ibxp-class="ibx-csl-next-btn"></div>
				</div>
				<div class="ibx-csl-page-markers" tabIndex="0" data-ibx-name="_pageMarkers" data-ibx-type="ibxHBox" data-ibxp-nav-key-root="true" data-ibxp-nav-key-auto-focus="true" data-ibxp-justify="center" data-ibxp-align="center">
				</div>
			</div>
		</markup-block>
	</markup>

