<ibx-res-bundle loadContext="bundle">
	<styles>
		<style-file src="idCanvas.css"/>
	</styles>
	<scripts>
		<script-file src="idCanvas.js" />		
	</scripts>
	
	<markup>
		<markup-block>
			<div class="canvas-panel">
				<div class="full-container" data-ibx-type="ibxVBox" data-ibxp-align="stretch">
					<div class="heading-area" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
						<div data-ibx-type="ibxLabel">Heading</div>
					</div>
					<div class="chart-area" data-ibx-type="ibxHBox" data-ibxp-align="stretch">
						<div class="chart-div" id='chart-div'></div>
						<div class="drop-label" data-ibx-type="ibxLabel" data-ibxp-glyph-classes="fa fa-level-down" 
							data-ibxp-icon-position="top">Drop measures and dimensions here</div>
					</div>
				</div>
			</div>
		</markup-block>
	</markup>
		
</ibx-res-bundle>